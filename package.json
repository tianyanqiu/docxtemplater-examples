{
  "name": "docxtemplater-examples",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/docxtemplater-examples.umd.production.js",
  "module": "dist/docxtemplater-examples.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "ts-lib-tools watch",
    "test": "ts-lib-tools test",
    "build": "ts-lib-tools build",
    "format": "ts-lib-tools format",
    "lint": "ts-lib-tools lint",
    "prepublishOnly": "ts-lib-tools build"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": "ts-lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "cross-env CI=true ts-lib-tools test",
      "eslint --fix --color",
      "git add"
    ]
  },
  "files": [
    "dist",
    "assets"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.4",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.12.14",
    "@types/pizzip": "^3.0.1",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-reconciler": "^0.18.0",
    "@types/react-test-renderer": "^16.9.1",
    "docxtemplater": "^3.15.0",
    "fs-extra": "^8.1.0",
    "pizzip": "^3.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-reconciler": "^0.24.0",
    "react-test-renderer": "^16.12.0"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "ts-lib-tools": "^0.5.2",
    "typescript": "^3.7.2"
  }
}
